{
    "name": "gdpro/gdpro-monolog",
    "description": "Monolog integration for ZF2",
    "type": "library",
    "license": "BSD-4-Clause",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Gary Gitton",
            "email": "contact@gary.pro",
            "homepage": "http://www.gary.pro"
        }
    ],
    "require": {
        "php": ">=5.5.8",
        "psr/log": "^1.0",
        "monolog/monolog": "^1.22"
    },
    "require-dev": {
        "zendframework/zend-stdlib": "^3.1",
        "zendframework/zend-modulemanager": "^2.7",
        "zendframework/zend-servicemanager": "^3.2",
        "zendframework/zend-eventmanager": "^3.1",
        "zendframework/zend-mvc": "^3.0",
        "phpunit/phpunit": "~4.3",
        "squizlabs/php_codesniffer": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "GdproMonolog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GdproMonologTest\\": "test/"
        }
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    }
}
