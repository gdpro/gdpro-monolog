{
    "name": "gdpro/gdpro-monolog",
    "description": "Monolog integration for ZF2",
    "type": "library",
    "license": "BSD-4-Clause",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Gary Gitton",
            "email": "contact@gary.pro",
            "homepage": "http://www.gary.pro"
        }
    ],
    "require": {
        "php": ">=5.5.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3",
        "monolog/monolog": "^1.22",
        "zend/zend-stdlib": "^3.1",
        "zend/zend-modulemanager": "^2.7",
        "zend/zend-servicemanager": "^3.2",
        "zend/zend-eventmanager": "^3.1",
        "zend/zend-mvc": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "GdproMonolog\\": "src/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "GdproMonologTest\\": "test/"
        }
    }
}
